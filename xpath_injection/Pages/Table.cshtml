@page
@model xpath_injection.Pages.TableModel
@{
}

<h2>XML Table</h2>
<table>
    <thead>
        <tr>
            <th>Column 1</th>
            <th>Column 2</th>
            <!-- Add more table headers as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model.TableData)
        {
            <tr>
                <td>@row.Element("Column1").Value</td>
                <td>@row.Element("Column2").Value</td>
                <!-- Add more table cells as needed -->
            </tr>
        }
    </tbody>
</table>